<section class="winter">
  <div class="container">
    <a class="back-link" href="/fr">&#8592; Retour</a>
    <h1>Wintershow 2020</h1>
    <h2>
      22 au 23 janvier 2020 <br />
      6490 Andermatt
    </h2>
  </div>
</section>

<section>
  <div class="container">
    <h2>Impressions</h2>
    <div class="cols">
      <% page.impressions.forEach(function(image) { %>
      <a href="#<%= image.name %>">
        <img src="<%= image.uri %>" />
      </a>
      <% }) %>
    </div>
  </div>
</section>

<!-- lightbox container hidden with CSS -->
<% page.impressions.forEach(function(image) { %>
<a href="#_" class="lightbox" id="<%= image.name %>">
  <img src="<%= image.uri %>" />
</a>
<% }) %>

<section>
  <div class="container">
    <h2>Exposants</h2>
    <ul class="no-bullets small-logos">
      <% page.exhibitors.forEach(function(image) { %>
      <li>
        <% if(image.link){ %>
        <a
          class="block"
          href="<%= image.link %>"
          target="_blank"
          rel="noopener noreferrer"
        >
          <img src="<%= image.uri %>" alt="<%= image.name %>" />
        </a>
        <% } else{ %>
        <img src="<%= image.uri %>" alt="<%= image.name %>" />
        <% } %>
      </li>
      <% }) %>
    </ul>
  </div>
</section>
